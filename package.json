{
  "name": "@gainium/websocket-connector",
  "version": "1.0.9",
  "description": "Websocket connector",
  "author": "Maksym Shamko",
  "private": true,
  "scripts": {
    "push": "git push origin main",
    "pull": "git pull origin main",
    "build": "(rm -rf dist || true) && tsc --project tsconfig.json",
    "main": "node -r dotenv/config dist/index.js",
    "price": "node -r dotenv/config dist/index.price.js",
    "lint": "tsc --noEmit --project tsconfig.json && eslint . --cache",
    "lint:fix": "eslint . --fix",
    "fullInit": "export GAINIUM_KUCOIN=@gainium/kucoin-api && (npm uninstall $GAINIUM_KUCOIN || true) && npm cache clean --force && npm install $GAINIUM_KUCOIN@git+https://github.com/Gainium/kucoin-api.git && unset GAINIUM_KUCOIN",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@types/amqplib": "^0.10.7",
    "@types/node": "^24.1.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@gainium/kucoin-api": "github:Gainium/kucoin-api",
    "amqplib": "^0.10.8",
    "axios": "^1.11.0",
    "binance": "^3.0.1",
    "bitget-api": "^2.3.6",
    "bybit-api": "^4.1.17",
    "coinbase-advanced-node": "^4.1.0",
    "dotenv": "^17.2.0",
    "isomorphic-ws": "^5.0.0",
    "okx-api": "^2.0.6",
    "redis": "^5.6.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^11.1.0",
    "ws": "^8.18.3"
  }
}
